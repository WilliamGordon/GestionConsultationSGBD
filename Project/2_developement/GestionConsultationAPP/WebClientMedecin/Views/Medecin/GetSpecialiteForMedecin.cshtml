@model IEnumerable<Models.Specialite>

@{
    ViewBag.Title = "GetSpecialiteForMedecin";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

@Html.Partial("ErrorNotificationView", new ViewDataDictionary { { "ErrorMessage", ViewBag.ErrorMessage } })

<h2>Mes spécialités</h2>

<p>
    <a href="/Medecin/AddSpecialiteForMedecin/@ViewBag.Medecin_ID">Ajouter une nouvelle spécialite</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
            </tr>
        }
    }
</table>
